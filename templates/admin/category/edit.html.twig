{% extends "admin/base.html.twig" %}

{% block title %}Édition Catégorie - {{ category.name }}{% endblock %}

{% block body %}
    <h1>Édition de la catégorie: {{ category.name }}</h1>

    {{ form_start(formView) }}
        {{ form_row(formView.name) }}
        <button type="submit" class="btn btn-primary">
            <i class="fas fa-save mr-1"></i>
            Enregistrer la modification
        </button>
    {{ form_end(formView) }}

{% endblock %}